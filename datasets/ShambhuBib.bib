doi = {10.1016/j.compscitech.2006.02.025},
file = {:C$\backslash$:/Users/SAM KUMAR/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhou et al. - 2006 - Analytic characterization of the permeability of dual-scale fibrous porous media.pdf:pdf},
isbn = {0266-3538},
issn = {02663538},
journal = {Composites Science and Technology},
keywords = {A. Fabrics/textiles,A. Short fiber composites,B. Modeling,B. Permeability,E. Resin transfer molding (RTM),F. Dual Scale porosity,Fiber tows,G. Darcy's law,I. Analytical model},
number = {15},
pages = {2795--2803},
pmid = {241654500021},

doi = {10.1016/j.matdes.2011.06.028},
file = {:C$\backslash$:/Users/SAM KUMAR/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhang et al. - 2011 - Qualitative separation of the effect of voids on the bending fatigue performance of hygrothermal conditioned carbo.pdf:pdf},
issn = {02641275},
journal = {Materials and Design},
keywords = {B. Laminates,E. Fatigue,F. Defects},
number = {10},

doi = {10.1016/j.compstruct.2016.06.040},
file = {:C$\backslash$:/Users/SAM KUMAR/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Butaud, Folt{\^{e}}te, Ouisse - 2016 - Sandwich structures with tunable damping properties On the use of Shape Memory Polymer as viscoelastic.pdf:pdf},
issn = {02638223},

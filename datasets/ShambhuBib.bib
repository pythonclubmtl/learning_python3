@article{Berges2016,
abstract = {This papers aims to characterize the influence of moisture uptake on the mechanical behaviour of unidirectional flax fibre-reinforced epoxy laminates. Monotonic and cyclic tensile tests and free vibration characterization are carried out. Results show that UD flax-epoxy composites, when exposed to hygrothermal conditioning at 70 °C and 85{\%} RH, exhibit a diffusion kinetic which follows a one dimensional Fickian behaviour. The mass uptake at equilibrium is approximately 3.3{\%} and the diffusion coefficient 6.5 × 10-6m2s-1. Water vapour sorption is shown to induce a significant change in the shape of the tensile stress-strain curve, a decrease in the dynamic elastic modulus of about 20{\%} and a 50{\%} increase in the damping ratio. Contrary to all expectations, water saturation does not degrade the monotonic tensile strength of such a flax-epoxy composites and leads to an increase in the fatigue strength for a high number of cycles.},
author = {Berges, Micha{\"{e}}l and L{\'{e}}ger, Romain and Placet, Vincent and Person, V{\'{e}}ronique and Corn, St{\'{e}}phane and Gabrion, Xavier and Rousseau, J{\'{e}}r{\^{o}}me and Ramasso, Emmanuel and Ienny, Patrick and Fontaine, St{\'{e}}phane},
doi = {10.1016/j.compositesa.2016.05.029},
file = {:C$\backslash$:/Users/SAM KUMAR/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Berges et al. - 2016 - Influence of moisture uptake on the static, cyclic and dynamic behaviour of unidirectional flax fibre-reinforced.pdf:pdf},
isbn = {1359-835x},
issn = {1359835X},
journal = {Composites Part A: Applied Science and Manufacturing},
keywords = {A. Natural fibres,A. Polymer-matrix composites,B. Mechanical properties,C. Moisture},
pages = {165--177},
title = {{Influence of moisture uptake on the static, cyclic and dynamic behaviour of unidirectional flax fibre-reinforced epoxy laminates}},
volume = {88},
year = {2016}
}

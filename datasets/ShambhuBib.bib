file = {:C$\backslash$:/Users/SAM KUMAR/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tan, Tong, Steven - 1999 - Micromechanics models for the elastic constants and failure strengths of plain weave composites.pdf:pdf},

title = {{Computational study of crack growth in SiC/Al composites}},
volume = {42},
year = {2005}
}
@article{Papakaliatakis2010,
abstract = {A displacement-based finite element-based numerical approach has been employed to study the damage growth in a unidirectional SiC/Al composite containing a pre-existing crack along the fibre/matrix interface. The composite is modeled as a two-material cylinder subjected to uniform displacement. A detailed analysis is made for the stress field in the vicinity of the debond crack tip. This approach incorporates an elastic–plastic analysis combined with a strain energy density criterion to predict debonded crack growth direction, extended stable growth and final termination. The influence of contact taking place between the debonded surfaces is also considered. It is shown that such surface contact leads to reduced stress and strain fields around the crack tip, while the extent of reduction is increased with debonding length. By combining the reduced stress field with the strain energy density criterion, a limiting value for the debonding extension can be calculated for the critical applied displacement that led to fibre fracture.},
author = {Papakaliatakis, G. and Karalekas, D.},
doi = {10.1016/j.tafmec.2010.03.005},
file = {:C$\backslash$:/Users/SAM KUMAR/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Papakaliatakis, Karalekas - 2010 - Damage growth by debonding in a single fibre metal matrix composite Elastoplasticity and strain energ.pdf:pdf},
issn = {01678442},
journal = {Theoretical and Applied Fracture Mechanics},
number = {2},
pages = {152--157},
title = {{Damage growth by debonding in a single fibre metal matrix composite: Elastoplasticity and strain energy density criterion}},
volume = {53},
year = {2010}
}
@article{Chiang2000,
abstract = {In this paper, the effect of interfacial debonding in the wake of a crack on the stress for matrix cracking is studied for unidirectional fiber reinforced ceramics. A simple shear-lag model is adopted to calculate the stress and strain fields in the fiber and the matrix. A criterion for crack-wake debonding is proposed by treating the debonding process as a particular crack propagation problem. Then, by using an energy balance approach the formulation of the matrix cracking stress of an infinite fiber-bridged crack is derived. The theoretical results are compared with experimental data of SiC/borosilicate, SiC/LAS and C/borosilicate ceramic composites.},
author = {Chiang, Yih-Cherng},
doi = {10.1016/S0013-7944(99)00130-7},

isbn = {978-1-62410-065-9},
journal = {AIAA/ASME/ASCE/AHS/ASC 46th SDMConference},
number = {April},
pages = {2005--2021},
title = {{3D Stress Analysis of Adhesively Bonded Composite Joints}},
url = {http://hypersizer.com/download.php?type=pdf{\&}file=AIAA2005SDM{\_}3D{\_}Stress{\_}Analysis{\_}of{\_}Adhesively{\_}Bonded{\_}Joints.pdf},

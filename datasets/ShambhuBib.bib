file = {:C$\backslash$:/Users/SAM KUMAR/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bonora, Ruggiero - 2006 - Micromechanical modeling of composites with mechanical interface - Part 1 Unit cell model development and manu.pdf:pdf},
isbn = {0266-3538},

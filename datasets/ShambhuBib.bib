@article{CharlesD.ShirrellandForrestA.Sandow,
abstract = {An experimental study of moisture diffusion in three high temperature epoxy resin matrix advanced composite materials - Narmco's T300-5208, Hercules' AS/3501-5, and AVCO's Boron (4 mil fiber)/5505 - was undertaken to aid in determining the amount of moisture that might be absorbed by an epoxy composite structural component during its service life and to evaluate potential accelerated environmental conditions for composite structural durability tests. The moisture absorption and desorption behavior was measured at 33 different hygrothermal conditions formed from combinations of 4 temperatures (70°F, 110°F, 145°F, and 180°F) and various humidities or water immersion. From this information the diffusivity, equilibrium solubility, swelling, and diffusion modes were determined. This study revealed the presence of absorption anomalies, history dependent sorption, water immersion damage, and hygrothermally induced microcracking. These phenomena were observed to occur only for certain combinations of hygrothermal environments/material systems. In addition, other experimental studies revealed the effects of postcure, resin content, void content, and an organic coating upon moisture diffusion in advanced composite epoxy resin matrix materials.},
author = {{Charles D. Shirrell and Forrest A. Sandow}},
journal = {Air Force Wright-Aeronautical Laboratories (AFSC)},
pages = {795--800},
title = {{The kinetics of moisture diffusion in three advanced composite epoxy resin matrix material systems}}

journal = {Composites Part B: Engineering},
number = {6},
pages = {446--453},
title = {{A critical reliability evaluation of fibre reinforced composite materials based on probabilistic micro and macro-mechanical analysis}},
volume = {41},
year = {2010}
}
@article{Xu2007,
abstract = {In this work, the effect of soaking and irradiating in praseodymium nitrate solution on the surface physicochemical properties of carbon fibers and interfacial properties of composites has been investigated, and the interfacial adhesion mechanism of treated carbon fiber/epoxy composite was analyzed. It was found that both of immersion and irradiation lead to an increase of fiber surface roughness, improvement of oxygen-containing groups, enhancement of disorder degree and introduction of praseodymium element on carbon fiber surface. As a result, the coordination linkage between fibers, praseodymium ion and matrix is formed and the interlaminar shear strength (ILSS) of composites is increased, due to the improvement of interfacial adhesion between fibers and matrix resin. Moreover, $\gamma$-ray irradiation treatment is superior to immersion treatment in promoting interfacial properties owing to the increase of carboxyl and carbonyl.},

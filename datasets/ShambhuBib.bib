file = {:C$\backslash$:/Users/SAM KUMAR/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shin et al. - 2001 - Thermal distortion analysis of orbiting solar array including degradation effects of composite materials.pdf:pdf},
